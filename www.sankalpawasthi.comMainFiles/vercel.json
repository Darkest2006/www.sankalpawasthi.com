{
  "version": 2,
  "builds": [
    {
      "src": "www.sankalpawasthi.com.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/*",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
